            <Paragraf id="Par1">
              <Explicatus>§ 1.</Explicatus>
              <Stk id="Par1_Stk1">
                <Exitus>
                  <Linea>
                    <Char>Staten yder for 2012 et tilskud på 624,1 mio. kr. til Færøernes hjemmestyre.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </Paragraf>
            <Paragraf id="Par2">
              <Explicatus>§ 2.</Explicatus>
              <Stk id="Par2_Stk1">
                <Exitus>
                  <Linea>
                    <Char>Tilskuddet udbetales forskudsvis i lige store rater hver måned.</Char>
                  </Linea>
                  <Linea>
                    <Char>Finansministeren kan dog efter aftale med Fær­øernes landsstyre fastsætte regler om ændrede udbetalingsterminer.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </Paragraf>
            <Paragraf id="Par3">
              <Explicatus>§ 3.</Explicatus>
              <Stk id="Par3_Stk1">
                <Exitus>
                  <Linea>
                    <Char>Loven træder i kraft dagen efter bekendtgørelsen i Lovtidende.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </Paragraf>
          </ParagrafGruppe>
