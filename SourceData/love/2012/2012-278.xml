  <TitelGruppe>
    <Titel>
      <Linea>
        <Char>Lov om ændring af tobaksafgiftsloven</Char>
      </Linea>
      <Nota formaNo="EU" id="Not1">
        <Explicatus>1)</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven har som udkast været notificeret i overensstemmelse med Europa-Parlamentets og Rådets direktiv 98/34/EF (informationsproceduredirektivet) som ændret ved direktiv 98/48/EF.</Char>
          </Linea>
        </Exitus>
      </Nota>
    </Titel>
    <UnderTitel>
      <Linea>
        <Char>(Fastsættelse af minimumsantal af cigaretter i cigaretpakker)</Char>
      </Linea>
    </UnderTitel>
  </TitelGruppe>
  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om tobaksafgifter (tobaksafgiftsloven), jf. lovbekendtgørelse nr. 635 af 21. august 1998, som ændret senest ved § 4 i lov nr. 1383 af 28. december 2011, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 3</Char>
                <Char> indsættes som </Char>
                <Char formaChar="Italic" signiChar="AendringURN">stk. 3:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Stk>
                <Explicatus>Stk. 3.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Pakninger med cigaretter skal indeholde mindst 20 cigaretter.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. april 2012.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk>
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven finder anvendelse for varer, som efter lovens ikrafttræden udleveres til forbrug fra registrerede virksomheder, angives til fortoldning eller importeres.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
