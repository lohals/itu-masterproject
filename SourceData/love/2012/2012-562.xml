        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>Seks steder i </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 24</Char>
                <Char> og i </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 25, stk. 2,</Char>
