    <IkraftCentreretParagraf>
      <Explicatus>§ 3</Explicatus>
      <Stk>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. juli 2012.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 4</Explicatus>
      <Stk>
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland, jf. dog stk. 2.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk>
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven kan ved kongelig anordning sættes helt eller delvis i kraft for Grønland med de ændringer, som de grønlandske forhold tilsiger.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
