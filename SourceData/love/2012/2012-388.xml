                <Char>og </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 8, stk. 2,</Char>
                <Char> ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="True">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>3.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char formaChar="Italic" signiChar="AendringURN">§ 9</Char>
