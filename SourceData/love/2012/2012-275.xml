  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I straffeloven, jf. lovbekendtgørelse nr. 1062 af 17. november 2011, som ændret ved § 22 i lov nr. 112 af 3. februar 2012, lov nr. 157 af 28. februar 2012, § 1 i lov nr. 158 af 28. februar 2012 og lov nr. 159 af 28. februar 2012, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 7, stk. 1, nr. 2, litra a,</Char>
                <Char> indsættes efter »seksuel udnyttelse af børn«: », menneskehandel«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>2.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 262 a, stk. 1,</Char>
                <Char> ændres »8 år« til: »10 år«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>3.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 262 a, stk. 1</Char>
                <Char> og </Char>
                <Char formaChar="Italic" signiChar="AendringURN">2,</Char>
                <Char> indsættes efter »slaveri eller slaverilignende forhold«: », strafbare handlinger«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft dagen efter bekendtgørelsen i Lovtidende.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 3</Explicatus>
      <Stk>
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
