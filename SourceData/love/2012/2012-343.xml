    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov nr. 106 af 26. februar 2008 om Arbejdsretten og faglige voldgiftsretter foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 2, stk. 1,</Char>
                <Char> ændres »31 suppleanter« til: »31-41 suppleanter«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
