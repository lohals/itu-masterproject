  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om Vækstfonden, jf. lovbekendtgørelse nr. 549 af 1. juli 2002, som ændret senest ved lov nr. 617 af 14. juni 2011, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 5</Char>
                <Char> indsættes som </Char>
                <Char formaChar="Italic" signiChar="AendringURN">stk. 3:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Stk>
                <Explicatus>Stk. 3.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Vækstfonden kan optage lån til finansiering af sine aktiviteter.</Char>
                  </Linea>
                  <Linea>
                    <Char>Det samlede lånebeløb eksklusive renter kan ikke uden godkendelse fra Folketingets Finansudvalg overstige 500 mio. kr.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. juni 2012.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
