  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I straffeloven, jf. lovbekendtgørelse nr. 1062 af 17. november 2011, som ændret ved § 22 i lov nr. 112 af 3. februar 2012, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 63, stk. 1, 1. pkt.,</Char>
                <Char> ændres »240« til: »300«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <AendringCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Exitus>
        <Linea>
          <Char>Loven træder i kraft den 1. marts 2012.</Char>
        </Linea>
      </Exitus>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 3</Explicatus>
      <Stk>
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
