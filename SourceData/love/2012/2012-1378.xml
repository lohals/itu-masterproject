    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om sygedagpenge, jf. lovbekendtgørelse nr. 653 af 26. juni 2012, som ændret ved § 15 i lov nr. 326 af 11. april 2012 og § 5 i lov nr. 928 af 18. september 2012, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 55, stk. 3, 1. pkt.,</Char>
                <Char> ændres »70 pct.« til: »80 pct.«</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
