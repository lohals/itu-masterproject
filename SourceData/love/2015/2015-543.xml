    <Titel id="id10b96508-b0a3-4db7-ae84-b205224d2409">
      <Linea>
        <Char>Lov om ændring af lov om anlæg af en jernbanestrækning København-Ringsted over Køge</Char>
      </Linea>
    </Titel>
    <UnderTitel id="id5c4bc384-67df-4abc-b41c-2bae902613ea">
      <Linea>
        <Char>(Bemyndigelse til etablering af niveaufri udfletning ved Ny Ellebjerg med dertilhørende anlæg)</Char>
      </Linea>
    </UnderTitel>
  </TitelGruppe>
  <DokumentIndhold id="ide171c575-35f3-4071-a4cc-1b05dee7be02">
    <Hymne id="id1168812d-d197-41d2-b6b6-4d0f0a760e31">
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf id="idd0603e73-be8b-4ad6-ba29-dcb3549aaea1">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov nr. 527 af 26. maj 2010 om anlæg af en jern­bane­stræk­ning København-Ringsted over Køge foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id0ce27941-ac73-4fa5-a9f7-60ca4843f7c2">
        <Explicatus>1.</Explicatus>
        <Aendring id="id2f135c12-f596-483b-9538-f8b9df0bda10">
          <AendringDefinition id="id8f7bfac6-bfc5-4ba1-bb93-8c291f97987f">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 1</Char>
                <Char> indsættes efter stk. 1 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id161a10fb-c0a7-4e4e-bb8e-854af521d5f5" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Stk id="id46f19fd7-db4b-40fb-9045-fa7e1a6d0d00">
                <Explicatus>Stk. 2.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Transportministeren bemyndiges til at etablere en niveaufri udfletning ved Ny Ellebjerg med dertilhørende anlæg, jf. stk. 3.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="ida580eea1-28d6-4959-9417-dd6b5dab920d">
          <Exitus>
            <Linea>
              <Char>Stk. 2 bliver herefter stk. 3.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer id="id90199598-300a-40b7-93c4-55768d673a7b">
        <Explicatus>2.</Explicatus>
        <Aendring id="idf72f855e-d877-4df2-90d5-6b43c2808959">
          <AendringDefinition id="id93b83a38-5498-469b-b732-d9dc0e3857df">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 1</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 4:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id26c41972-3802-4715-a741-2c673301cb48" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Stk id="id42f1c16e-ea7b-4081-8fdd-5212d2f58b14">
                <Explicatus>Stk. 4.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Kort over niveaufri udfletning ved Ny Ellebjerg fremgår af bilag 1 a.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id470766f6-c76d-4389-b072-97489bff950e">
        <Explicatus>3.</Explicatus>
        <Aendring id="id9a7c04da-7e11-402b-831e-4fae785feaa9">
          <AendringDefinition id="id34bb5ac6-ca5a-427a-a6a6-7da9d78265c7">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 2, 2. pkt.,</Char>
                <Char> ændres »bilag 2 og 3« til: »bilag 2-5«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idb6072032-a5f7-4200-8388-7aaceb3e2999" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id0eb3f3a2-5544-47d3-ac12-42d7c5942842">
        <Explicatus>4.</Explicatus>
        <Aendring id="idad8349dd-6c05-4054-8d87-224187569258">
          <AendringDefinition id="id494301a6-3b5e-4cc5-9c35-d1736c8b1748">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 4, stk. 2,</Char>
                <Char> ændres »med deraf følgende rådighedsindskrænkninger« til: »eller andre servitutter med deraf følgende rådighedsindskrænkninger, der er nødvendige for etableringen af anlægget«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id2e75660d-f577-40a0-bdfd-03df47de0551" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="idfa2548ba-b0fc-447f-b171-33e29309d2bd">
        <Explicatus>5.</Explicatus>
        <Aendring id="idd3a4e27d-1ab3-472e-8ec7-7b4c25bb6c86">
          <AendringDefinition id="id6abe90b3-9e00-4ec2-aa7d-079aa4a01b06">
            <Exitus>
              <Linea>
                <Char>Som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">bilag 1 a, 4</Char>
                <Char> og </Char>
                <Char signiChar="AendringURN" formaChar="Italic">5</Char>
                <Char> indsættes bilag 1, 2 og 3 til denne lov.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id2e1a265f-92a1-4a63-8b2f-94d46af07b3b" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="id39d928c2-c35f-40d4-9df8-9b40e31decc9">
      <Explicatus>§ 2</Explicatus>
      <Stk id="ida64e35ea-6b51-4078-bf5d-72233be90fa6">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft dagen efter bekendtgørelsen i Lovtidende.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
