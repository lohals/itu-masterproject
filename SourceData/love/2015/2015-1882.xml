      <AendringsNummer id="idf3d06535-9ec2-4128-bdd3-8410673ea29e">
        <Explicatus>7.</Explicatus>
        <Aendring id="ida145c8cc-5eca-45d2-979d-a441e312e69c">
          <AendringDefinition id="id01dbfa15-535a-451a-ba72-330b7640aa67">
