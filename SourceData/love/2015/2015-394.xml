          <AendringDefinition id="id33829b4a-b145-447d-9afe-47c0fd3bc8f6">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 44 a, stk. 1, nr. 2, </Char>
                <Char>ophæves, og i stedet indsættes:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id958acce5-fc51-4a99-875f-f0cc832158f7" punktumEfterSlutcitationstegn="True">
            <AendringNyTekst>
              <Indentatio id="id13dd76aa-170c-497c-ae93-4a7865ffcad8" formaInd="Nummer">
                <Explicatus>2)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>at udlændingen er meddelt tidsubegrænset opholdstilladelse eller afslag herpå,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
              <Indentatio id="id2e469c05-50b4-4ec5-85a3-e716921e6d93" formaInd="Nummer">
                <Explicatus>3)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>at udlændingen er meddelt tidsbegrænset opholdstilladelse efter afslag på tidsubegrænset opholdstilladelse, jf. nr. 2, eller</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
            </AendringNyTekst>
          </AendringAktion>
