            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 9, stk. 6, stk. 15, 3. pkt., stk. 20, 3. pkt., </Char>
                <Char>og </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 27, 3. pkt., </Char>
                <Char>og </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 19, stk. 1, nr. 6, 1. pkt.,</Char>
                <Char> og to steder i </Char>
                <Char signiChar="AendringURN" formaChar="Italic">nr. 7, 1. pkt., </Char>
                <Char>ændres »sin egen bolig« til: »en selvstændig bolig«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id9007f6cf-8bc7-403e-87e4-bcd019181b3d" punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
