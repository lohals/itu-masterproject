    <Ikraft id="id80c6b12c-2b00-4009-b70a-9eb79a999957">
      <Exitus>
        <Linea>
          <Char>Lov nr. 500 af 12. juni 2009 (Påbudsordning, betaling for opfølgende besætningskontrol og tematiserede kontroller m.v.)</Char>
        </Linea>
        <Nota id="id171a5bdd-666a-4027-bd50-617ff173b667">
          <Explicatus>5)</Explicatus>
          <Exitus>
            <Linea>
              <Char>Lovændringen vedrører § 15 a og § 16, stk. 1.</Char>
            </Linea>
          </Exitus>
        </Nota>
        <Linea>
          <Char>indeholder følgende ikrafttrædelsesbestemmelse:</Char>
        </Linea>
      </Exitus>
      <IkraftCentreretParagraf id="id48849869-b22b-4e94-b917-a8d48e019e78">
        <Explicatus>§ 7</Explicatus>
        <Stk id="id32413df3-de83-4e59-8f8d-193422be5dbc">
          <Explicatus>Stk. 1. </Explicatus>
          <Exitus>
            <Linea>
              <Char>Loven træder i kraft den 1. juli 2009, jf. dog stk. 2.</Char>
