          <AendringDefinition id="idc6dc2622-7b62-4c95-b8da-614e6e0c8a00">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 36, stk. 1,</Char>
                <Char> indsættes efter nr. 10 som nyt nummer:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id2ba24ff1-0b6b-4c25-a7c5-dd54f6f7b6d8">
            <AendringNyTekst>
              <Indentatio id="id99345526-ffa4-4768-8977-04c6d91a85a3" formaInd="Nummer">
                <Explicatus>11)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Genoptagelse af helårsbeboelse af boliger, som midlertidigt har været anvendt til fritidsformål i henhold til et af kommunalbestyrelsen meddelt samtykke i medfør af lov om midlertidig regulering af boligforholdene.</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="idf69590ec-15c6-4cf4-a8b9-ab8e2f97ed9e">
          <Exitus>
            <Linea>
              <Char>Nr. 11-15 bliver herefter nr. 12-16.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
