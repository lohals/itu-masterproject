    <AendringCentreretParagraf id="id70026d59-4a71-451b-aed8-6e09c4cd98a8">
      <Explicatus>§ 2</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om midlertidig regulering af boligforholdene, jf. lovbekendtgørelse nr. 810 af 1. juli 2015, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="idd45c8f26-001c-4362-ac05-f55e67c9df2b">
        <Explicatus>1.</Explicatus>
        <Aendring id="id323ec3b2-00d2-49c5-8c34-736f6312ebc1">
          <AendringDefinition id="id2cba9ac1-9df4-4d2a-9442-c2f1de4a23ed">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 50, stk. 2, </Char>
                <Char>udgår »af boligens ejer« og »af den samme ejer«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idb6ff45b9-9ab0-4209-b9b3-04f0c7e4060b">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id6d614ece-514a-4dfb-8411-6d320d844e16">
        <Explicatus>2.</Explicatus>
        <Aendring id="id98731cdb-a2be-4823-821f-8b22dcea24be">
          <AendringDefinition id="id0bddff56-3636-4b07-9c58-2099560667b1">
            <Exitus>
              <Linea>
                <Char>I</Char>
                <Char formaChar="Italic"> § 50, stk. 2,</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">2. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ida003ad57-e2e8-48ee-91f0-891d4fc74327">
            <AendringNyTekst>
              <Exitus>
                <Linea>
                  <Char>Kommunalbestyrelsen kan meddele bindende forhåndssamtykke.</Char>
                </Linea>
              </Exitus>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
