        <Aendring id="id34c7f371-8935-428c-a9b6-f44cbfb33f7b">
          <AendringDefinition id="id88b45688-8ef2-4927-a0ba-573b723349d1">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 36, stk. 1, nr. 9, </Char>
                <Char>indsættes efter »overstiger 250 m</Char>
                <Char formaChar="Superscript">2</Char>
                <Char>«: », jf. dog stk. 6«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idd3ef918e-7842-4ee2-af64-2d21ee3195fa">
            <AendringNyTekst />
