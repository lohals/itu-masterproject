            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>6.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 79</Char>
                <Char> indsættes efter stk. 1 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Stk>
                <Explicatus>Stk. 2.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Miljøministeren kan fastsætte regler om inddragelse af offentligheden i forbindelse med udarbejdelse af eksterne beredskabsplaner for listevirksomheder, jf. § 35 d.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul>
          <Exitus>
            <Linea>
              <Char>Stk. 2 og 3 bliver herefter stk. 3 og 4.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>7.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 79, stk. 3,</Char>
                <Char> der bliver stk. 4, ændres »1 og 2« til: »1-3«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
