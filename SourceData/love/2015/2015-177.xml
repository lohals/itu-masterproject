            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 45 d, stk. 2,</Char>
                <Char> udgår », herunder formen herfor«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
