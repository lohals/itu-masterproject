    <IkraftCentreretParagraf id="id8c1da529-c110-495a-a7d1-31fc3a1938e1">
      <Explicatus>§ 2</Explicatus>
      <Stk id="id08607215-ca46-447b-94f3-2fbd44f2e228">
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft dagen efter bekendtgørelsen i Lovtidende, jf. dog stk. 2.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk id="id5196a7b1-dd22-409a-866d-70d3d07aafe1">
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Udlændingelovens §§ 37 h, 37 i og 37 k som affattet ved denne lovs § 1, nr. 12, ophæves den 1. december 2019.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk id="id53d74e7c-3047-43dc-a540-919f38cec571">
        <Explicatus>Stk. 3.</Explicatus>
