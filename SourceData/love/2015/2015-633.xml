      <AendringsNummer id="idbc71d909-ed5d-40b7-b260-9c2b40cc84b9">
        <Explicatus>10.</Explicatus>
        <Aendring id="id30c13117-929a-40f9-8445-6faae7d7a581">
          <AendringDefinition id="id892a60f8-d0cb-4a6a-bd49-dd3105405ee0">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 25, stk. 1-4, </Char>
