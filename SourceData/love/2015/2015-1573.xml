          <AendringDefinition id="id97f68246-37dd-4eb3-91fc-5e45dfb6444f">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 45, stk. 1, nr. 2, </Char>
                <Char>indsættes efter »§ 28, stk. 2,«: »§ 28 a, stk. 2,«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
