    <AendringCentreretParagraf>
      <Explicatus>§ 3</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov nr. 106 af 26. februar 2008 om Arbejdsretten og faglige voldgiftsretter, som ændret ved lov nr. 343 af 17. april 2012, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 3, stk. 1, nr. 2,</Char>
                <Char> ændres »Sammenslutningen af Landbrugets Arbejdsgiverforeninger« til: »Dansk Arbejdsgiverforening«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <AendringCentreretParagraf>
      <Explicatus>§ 4</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov nr. 286 af 24. april 1996 om brug af helbredsoplysninger m.v. på arbejdsmarkedet foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 8, stk. 2,</Char>
                <Char> ændres »16 medlemmer« til: »15 medlemmer«, og »og 1 repræsentant for Sammenslutningen af Landbrugets Arbejdsgivere« udgår.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
