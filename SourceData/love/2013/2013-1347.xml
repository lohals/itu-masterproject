    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I fusionsskatteloven, jf. lovbekendtgørelse nr. 1120 af 14. november 2012, som ændret ved § 4 i lov nr. 433 af 16. maj 2012, § 2 i lov nr. 1255 af 18. december 2012, § 2 i lov nr. 1402 af 23. december 2012 og § 19 i lov nr. 600 af 12. juni 2013, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 6, stk. 4,</Char>
                <Char> ændres », andelskasser og sammenslutninger af andelskasser efter §§ 89-96 i lov om finansiel virksomhed« til: »og andelskasser«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>2.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 14, nr. 3,</Char>
                <Char> udgår »og sammenslutninger af andelskasser efter §§ 89-96 i lov om finansiel virksomhed«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>3.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 14 g, stk. 1,</Char>
                <Char> udgår »og sammenslutninger heraf«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
