  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om Arbejdsgivernes Uddannelsesbidrag, jf. lovbekendtgørelse nr. 203 af 25. februar 2013, som ændret ved § 6 i lov nr. 895 af 4. juli 2013, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic">§ 1, stk. 2,</Char>
                <Char> ændres »lov om godtgørelse ved deltagelse i erhvervsrettet voksen- og efteruddannelse samt befordringsudgifter« til: »lov om godtgørelse og tilskud til befordring ved deltagelse i erhvervsrettet voksen- og efteruddannelse«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion />
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>2.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char formaChar="Italic">§ 3, stk. 1, 3. pkt.,</Char>
                <Char> ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion />
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>3.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic">§ 25</Char>
                <Char> indsættes efter stk. 2 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst>
              <Stk>
                <Explicatus>Stk. 3.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Revisionen foretages i overensstemmelse med § 3 i lov om revisionen af statens regnskaber m.m.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul>
          <Exitus>
            <Linea>
              <Char>Stk. 3-5 bliver herefter stk. 4-6.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2014.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk>
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven har virkning fra og med regnskabsåret 2014.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
