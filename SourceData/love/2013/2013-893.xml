    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om almene boliger m.v., jf. lovbekendtgørelse nr. 884 af 10. august 2011, som ændret bl.a. ved lov nr. 1363 af 28. december 2011 og lov nr. 556 af 18. juni 2012 og senest ved § 22 i lov nr. 493 af 21. maj 2013, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 91, stk. 1,</Char>
                <Char> indsættes efter 3. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst>
              <Exitus>
                <Linea>
                  <Char>I 2013 forhøjes den investeringsramme, der er nævnt i 1. pkt., med 4.000 mio. kr.</Char>
                </Linea>
              </Exitus>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
