  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>Lov om tilskud til vikarordninger inden for jordbruget, jf. lovbekendtgørelse nr. 875 af 17. december 1991, ophæves.</Char>
        </Linea>
      </Exitus>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2012.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk>
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Tilsagn om tilskud meddelt før lovens ikrafttræden behandles efter de hidtil gældende regler om tilskud til vi­kar­ordninger inden for jordbruget.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
