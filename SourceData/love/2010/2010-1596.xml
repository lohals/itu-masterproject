    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om aktiv socialpolitik, jf. lovbekendtgørelse nr. 946 af 1. oktober 2009, som ændret bl.a. ved § 3 i lov nr. 431 af 28. april 2010 og senest ved § 4 i lov nr. 573 af 31. maj 2010, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 13, stk. 3, 2. pkt.,</Char>
                <Char> udgår »er under 30 år og«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>2.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 51, stk. 1,</Char>
                <Char> ændres »§ 28, stk. 3« til:</Char>
              </Linea>
              <Linea>
                <Char>»§ 28, stk. 2«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst />
