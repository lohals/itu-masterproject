      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om folkekirkens økonomi, jf. lovbekendtgørelse nr. 583 af 31. maj 2010, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 21 b, stk. 2,</Char>
                <Char> udgår »bortset fra gravstedskapitaler«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Stk>
