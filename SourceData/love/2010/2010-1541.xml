    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov nr. 399 af 7. juni 1989 om anvendelse af Christianiaområdet, som ændret ved lov nr. 431 af 9. juni 2004, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char formaChar="Italic" signiChar="AendringURN">§ 11, stk. 1, nr. 1,</Char>
                <Char> affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst>
              <Exitus>
                <Index>
                  <Indentatio formaInd="Nummer">
                    <Explicatus>1)</Explicatus>
                    <Exitus>
                      <Linea>
                        <Char>uden forsvarsministerens tilladelse foretager de i § 3, stk. 2 og 3, nævnte handlinger,</Char>
                      </Linea>
                    </Exitus>
                  </Indentatio>
                </Index>
              </Exitus>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
