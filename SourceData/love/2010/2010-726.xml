  <DokumentIndhold>
    <Hymne>
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf>
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>Lov nr. 1222 af 27. december 2003 om Banedanmark ophæves.</Char>
        </Linea>
      </Exitus>
    </AendringCentreretParagraf>
    <AendringCentreretParagraf>
      <Explicatus>§ 2</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om jernbane, jf. lovbekendtgørelse nr. 969 af 8. oktober 2009, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer>
        <Explicatus>1.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>Efter § 11 indsættes i </Char>
                <Char formaChar="Italic" signiChar="AendringURN">kapitel 5:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion>
            <AendringNyTekst>
              <Paragraf>
                <Explicatus>§ 11 a. </Explicatus>
                <Stk>
                  <Exitus>
                    <Linea>
                      <Char>Banedanmark kan forsyne jernbanevirksomhederne med kørestrøm efter nærmere regler, der fastsættes af transportministeren.</Char>
                    </Linea>
                    <Linea>
                      <Char>Lov om elforsyning finder ikke anvendelse på Banedanmarks elforsyningsaktiviteter.</Char>
                    </Linea>
                  </Exitus>
                </Stk>
              </Paragraf>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf>
      <Explicatus>§ 3</Explicatus>
      <Stk>
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft dagen efter bekendtgørelsen i Lovtidende.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk>
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Bekendtgørelse nr. 543 af 24. juni 2004 om Banedanmarks elforsyningsvirksomhed forbliver i kraft, indtil den ophæves eller erstattes af regler fastsat i medfør af lov om jernbane.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
