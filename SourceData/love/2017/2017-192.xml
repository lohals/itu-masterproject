        <Explicatus>13.</Explicatus>
        <Aendring id="id00d35adb-44aa-4ded-a50f-fa4175f6a6d9">
          <AendringDefinition id="ida4828082-1978-4535-b955-554a7934f8f1">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 45 </Char>
                <Char>indsættes efter stk. 3 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idf61ca482-2eca-4012-a28d-f88e39b44f5c">
            <AendringNyTekst>
              <Stk id="ide1eee850-831b-4b4d-837a-856c714a4257">
                <Explicatus>Stk. 4.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>I forbindelse med planlægningen af det kommende skoleår forelægger skolens leder det kommende skoleårs skemaer for undervisningen og rammerne for skiftende skemaer for skolebestyrelsen, med henblik på at skolebestyrelsen kan afgive en udtalelse herom.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="idcecc67fb-0ec1-489a-a109-d27c2c27fbd6">
