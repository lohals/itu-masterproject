                <Char signiChar="AendringURN" formaChar="Italic">8:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id2478d335-ee4f-4e09-81e1-552ac8ed6852">
            <AendringNyTekst>
              <Exitus id="id8ecb2fff-fa28-4d13-bf19-14b72ff3a29d">
                <Index>
                  <Indentatio id="idecedc4e8-2687-410f-958c-a8f1b3a5007c" formaInd="Nummer">
                    <Explicatus>7)</Explicatus>
                    <Exitus>
                      <Linea>
                        <Char>Afslag på opholdstilladelse efter § 9 c, stk. 3.</Char>
