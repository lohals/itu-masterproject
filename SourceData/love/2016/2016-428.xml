      </AendringsNummer>
      <AendringsNummer id="id4bbbfb97-e273-423f-81fb-ff85ca8e882e">
        <Explicatus>2.</Explicatus>
        <Aendring id="idc89e3673-ce06-4f4d-8979-c92633681d33">
          <AendringDefinition id="id2ea23805-28a6-488e-b501-c9aa9951684a">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 56, nr. 4,</Char>
