        <Aendring id="ida141c829-8fa3-488f-9898-3fd5fbe85dc5">
          <AendringDefinition id="id9bd821d6-360c-4afd-b12f-1b2bd8df8e18">
            <Exitus>
              <Linea>
                <Char>Efter § 15 indsættes:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
