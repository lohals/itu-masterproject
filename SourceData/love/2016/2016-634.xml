          <AendringDefinition id="id06c306f2-87ae-462b-8218-fa65202a5720">
            <Exitus>
              <Linea>
                <Char>Efter § 37 indsættes i </Char>
                <Char signiChar="AendringURN" formaChar="Italic">kapitel 11:</Char>
              </Linea>
