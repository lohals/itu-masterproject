    <IkraftCentreretParagraf id="idafddd53d-4155-4142-92bb-af973477a9f0">
      <Explicatus>§ 5</Explicatus>
      <Stk id="id34bcb051-4a33-4741-a439-a1126dc0caf5">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. juli 2016.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="idac048b92-e095-4568-80f9-002af9d9148a">
      <Explicatus>§ 6</Explicatus>
      <Stk id="id569c3ae8-6db3-4498-a4a7-46c9d8473133">
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland, men kan ved kongelig anordning helt eller delvis sættes i kraft for Grønland med de ændringer, som de grønlandske forhold tilsiger.</Char>
          </Linea>
        </Exitus>
      </Stk>
