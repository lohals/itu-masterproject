    <AendringCentreretParagraf id="ida77e8711-7dce-4e8b-8ab0-f173185be445">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om hjemmeværnet, jf. lovbekendtgørelse nr. 198 af 9. februar 2007, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id8124215a-c881-4b3f-8e67-b84411eca047">
        <Explicatus>1.</Explicatus>
        <Aendring id="idfc8a7970-d239-47fb-afcf-6e276a5e3fad">
          <AendringDefinition id="id7e41ac42-0167-4a5b-8574-d313863429d3">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 2, stk. 4,</Char>
                <Char> udgår », Marinehjemmeværnsinspektoratet og Flyverhjemmeværnsinspektoratet«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idad74e47b-2eb4-415c-9ac1-419d10f705a9">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="ide4cd64ea-0773-4dbb-a527-161ed156c3ee">
      <Explicatus>§ 2</Explicatus>
      <Stk id="id46ca177a-cdcb-445c-94b9-567a3b12b9d7">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2017.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="ide52981cb-02c8-4cbc-b00d-e08af4f590ad">
      <Explicatus>§ 3</Explicatus>
      <Stk id="id428644bb-16bd-4682-b20e-e1f12404921b">
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland, men kan ved kongelig anordning helt eller delvis sættes i kraft for Færøerne og Grønland med de ændringer, som de færøske og grønlandske forhold tilsiger.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
