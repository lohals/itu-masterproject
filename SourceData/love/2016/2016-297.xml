          <AendringDefinition id="id035074c7-8738-4048-a3ad-95cbe359838f">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 1, stk. 1, </Char>
                <Char>ændres »jf. dog § 2« til: »jf. bilag A«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id6ddeadf3-92a8-408f-9972-cefabb24ee0d">
