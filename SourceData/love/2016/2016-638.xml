    <AendringCentreretParagraf id="id0610c30b-fbd1-4b5c-b1b9-9dc98a3a0544">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om forsikringsaftaler, jf. lovbekendtgørelse nr. 1237 af 9. november 2015, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id97d7fa90-8539-4655-bd9a-b8f5799baae2">
        <Explicatus>1.</Explicatus>
        <Aendring id="ide62aa8b6-a2b9-4769-a99d-abfd2e7522db">
          <AendringDefinition id="id96f710b1-7dbf-485c-93b7-8eb90d5eb3b0">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 3 a, 2. pkt.,</Char>
                <Char> udgår »eller andre personers«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id5f853775-93f6-45ce-b604-efd0fbfb1117">
            <AendringNyTekst />
