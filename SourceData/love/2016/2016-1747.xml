        <Aendring id="id6e83695e-aa38-4a9e-b422-176e357bac40">
          <AendringDefinition id="id69c69c63-e399-4d70-a50e-9cff8edd020b">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 18, stk. 2, 4. pkt., </Char>
                <Char>ophæves, og i stedet indsættes:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ide5a6832b-1148-4899-a12e-c2835d195ea0">
            <AendringNyTekst>
              <Stk id="id43c381b1-71e7-4b94-9b58-f891123bbd9b">
                <Exitus>
                  <Linea>
                    <Char>I 2017 udgør bidraget 455 kr. pr. fuldtidsbeskæftiget i 2017-pris- og -lønniveau.</Char>
                  </Linea>
                  <Linea>
                    <Char>Herefter udgør bidraget 525 kr. årligt pr. fuldtidsbeskæftiget i 2017-pris- og -lønniveau.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
