        <Aendring id="idfc720f6b-5747-44d9-b1a1-328ab5399e3f">
          <AendringDefinition id="id757f3c93-6c12-42ff-b5c9-cfe0f6c3968f">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 18, stk. 1, 4. pkt., </Char>
                <Char>ophæves, og i stedet indsættes:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idcf82fe99-a48b-48b0-974d-c504ad56049a">
            <AendringNyTekst>
              <Stk id="idd5869e76-929e-40ab-8304-5363cd7631b7">
                <Exitus>
                  <Linea>
                    <Char>I 2017 udgør bidraget 2.382 kr. pr. fuldtidsbeskæftiget i 2017-pris- og -lønniveau.</Char>
                  </Linea>
                  <Linea>
                    <Char>Herefter udgør bidraget 2.312 kr. årligt pr. fuldtidsbeskæftiget i 2017-pris- og -lønniveau.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id30dda2e6-39dc-4566-808f-06aa1849bcb3">
