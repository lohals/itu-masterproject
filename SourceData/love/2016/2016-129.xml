      <AendringsNummer id="idafcd9404-020b-4479-bdac-80d2dd7fcc02">
        <Explicatus>20.</Explicatus>
        <Aendring id="id59775f42-730d-41be-942e-c8c700136389">
          <AendringDefinition id="id6402d79c-dd39-4492-9e60-12ed65bc0d49">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 272, stk. 2,</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">2. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id556f7915-d008-4e25-8f57-d13f6dba48bb">
            <AendringNyTekst>
              <Stk id="id2efa864e-a8c0-49f5-8dab-a10f4b5aa0ec">
                <Exitus>
                  <Linea>
                    <Char>Styrelsen for Patientsikkerhed kan ligeledes i forskrifter om offentliggørelse af afgørelser, der udstedes i medfør af § 215 b, stk. 3, fastsætte bestemmelser om straf i form af bøde for overtrædelse af bestemmelserne i forskrifterne.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
