          <AendringDefinition id="idb45672cb-87e7-4357-8ff7-2d253da4bad8">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 215 b </Char>
                <Char>indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 3:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idb41fc990-983e-4c59-9615-3d43b4ca9023">
            <AendringNyTekst>
              <Stk id="iddc971c41-7799-4160-9fca-72fcff8f3d2f">
                <Explicatus>Stk. 3.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Styrelsen for Patientsikkerhed fastsætter regler om, at sygehuse, klinikker, praksisser el.lign.</Char>
                  </Linea>
                  <Linea>
                    <Char>skal offentliggøre Styrelsen for Patientsikkerheds afgørelser efter stk. 1.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
