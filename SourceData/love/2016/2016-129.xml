    <IkraftCentreretParagraf id="idb82064bb-1acf-4f89-bb8c-52b72b7f31bf">
      <Explicatus>§ 2</Explicatus>
      <Stk id="idf2b805eb-bb98-4d0b-9421-9b8332151921">
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. juli 2016.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk id="idc4b9ea5e-d5ce-4603-905b-e0e5515ebe0b">
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>§ 1, nr. 18, finder tilsvarende anvendelse for afgørelser, der er truffet før den 1. juli 2016, og som fortsat er gældende, når de administrative forskrifter udstedt af Styrelsen for Patientsikkerhed efter sundhedslovens § 215 b, stk. 3, træder i kraft.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="id2a7804e4-f116-4ef0-8840-50490877b894">
      <Explicatus>§ 3</Explicatus>
      <Stk id="id459bb571-dc3b-4215-aca6-23cd604117c3">
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland, jf. dog stk. 2.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk id="id767f3622-99b5-42b4-a069-8de402419da4">
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>§ 1, nr. 18 og 20, kan ved kongelig anordning sættes helt eller delvis i kraft for Færøerne med de ændringer, som de færøske forhold tilsiger.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
