      <AendringsNummer id="id51da5d9e-bed8-425a-9243-9ca15b55eb43">
        <Explicatus>11.</Explicatus>
        <Aendring id="id334e9d02-df35-43ed-b75b-d855fe82872e">
          <AendringDefinition id="idf9a807db-f86e-4789-83c8-f53059436dc4">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 171</Char>
                <Char> indsættes efter stk. 1 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ida868221c-aec4-469d-8a55-428d70e2608b">
            <AendringNyTekst>
              <Stk id="id190f13c5-0768-4751-8400-048ceb6ea936">
                <Explicatus>Stk. 2.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Regionsrådet yder befordring eller befordringsgodtgørelse til personer, som er henvist til diagnostisk undersøgelse på sygehus, jf. § 79, til brug for udredning hos alment praktiserende læge, jf. § 60, eller hos praktiserende speciallæge, jf. § 64.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="id9073e7c0-b719-4302-933d-841d7b2a77b1">
          <Exitus>
            <Linea>
              <Char>Stk. 2-4 bliver herefter stk. 3-5.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
