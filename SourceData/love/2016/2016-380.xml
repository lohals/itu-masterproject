        <Aendring id="idf01fff7f-8f7a-4ae9-afc2-27f59d18d7a5">
          <AendringDefinition id="id21489b90-cbac-40b0-a65f-0c758d013430">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 47, stk. 1</Char>
                <Char> og </Char>
                <Char signiChar="AendringURN" formaChar="Italic">2,</Char>
                <Char> ændres »Ministeren for børn, ligestilling, integration og sociale forhold fastsætter efter forhandling med beskæftigelsesministeren« til: »Beskæftigelsesministeren fastsætter efter forhandling med udlændinge-, integrations- og boligministeren«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id93303a52-50b1-4d9d-af82-805796fd6e1b">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <AendringCentreretParagraf id="id89d3209f-b3cf-4717-aef7-2f89b8515341">
      <Explicatus>§ 3</Explicatus>
      <Exitus>
        <Linea>
          <Char>I repatrieringsloven, jf. lovbekendtgørelse nr. 1099 af 7. oktober 2014, foretages følgende ændring:</Char>
