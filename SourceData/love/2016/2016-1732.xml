          <AendringDefinition id="idad53e91d-59ea-42a7-b404-678585d1bb71">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 6, stk. 1, </Char>
