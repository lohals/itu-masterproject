      <AendringsNummer id="id3ec426dd-8113-40af-a146-0a7246f06174">
        <Explicatus>14.</Explicatus>
        <Aendring id="id726f2c25-438f-470f-b4ef-ee46c8813e68">
          <AendringDefinition id="id05911fe7-9773-4516-8462-c5856e02dc80">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 8, stk. 1, </Char>
                <Char>ændres »vedtægten« til: »medlemspligt af laget«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
