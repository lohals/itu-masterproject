              <Linea>
                <Char>Overskriften til </Char>
                <Char signiChar="AendringURN" formaChar="Italic">kapitel 4</Char>
                <Char> affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id7d4f0512-c4d6-4254-b826-38093d69fafa" punktumEfterSlutcitationstegn="True">
            <AendringNyTekst>
