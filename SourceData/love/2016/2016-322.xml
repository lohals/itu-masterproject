          <AendringDefinition id="id04e752ec-047d-4c7a-8a51-095a080d93e2">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 7, stk. 1, </Char>
                <Char>udgår »eller på frilager«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id99676de5-207c-4d2a-8d16-7d4b4538a06e">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
