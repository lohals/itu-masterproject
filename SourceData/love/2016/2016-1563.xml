        <Explicatus>6.</Explicatus>
        <Aendring id="id437f3752-c910-481e-81aa-b45f0da2d3b3">
          <AendringDefinition id="id70fd5f37-5f33-43b4-972d-80d1aa584cd0">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 9 b, stk. 1, 1. pkt.,</Char>
