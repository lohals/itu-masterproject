        <Aendring id="ideddbf709-03e1-447b-a836-4d4b30510a49">
          <AendringDefinition id="idd6fcafe0-bd1f-4b1d-ad71-59c04ff6d18a">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 47, stk. 9, 1. </Char>
                <Char>og</Char>
                <Char signiChar="AendringURN" formaChar="Italic"> 2. pkt.,</Char>
                <Char> der bliver stk. 8, 1. og 2. pkt., ændres »stk. 8« til: »stk. 7«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ida4e55a26-562f-408d-bcb4-9d6d96297ddb">
            <AendringNyTekst />
