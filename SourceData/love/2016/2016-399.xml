        <Aendring id="id9e313d7e-7d75-4aa2-baf6-61f603c53dca">
          <AendringDefinition id="ide41d5cc3-f782-42b6-b807-829d94acc8e1">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 52, stk. 4, 1. pkt.,</Char>
