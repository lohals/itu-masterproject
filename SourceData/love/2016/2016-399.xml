    <AendringCentreretParagraf id="id3bf800a1-ddc1-4297-ba6e-ea0566e089da">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om fremme af vedvarende energi, jf. lovbekendtgørelse nr. 122 af 6. februar 2015, som ændret bl.a. ved lov nr. 900 af 4. juli 2013, § 1 i lov nr. 744 af 1. juni 2015 og lov nr. 1877 af 29. december 2015 og senest ved § 8 i lov nr. 261 af 16. marts 2016, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="idad7bdb13-8ab5-481a-9b25-1d99049a369c">
        <Explicatus>1.</Explicatus>
        <Aendring id="id7502993e-de59-4709-b6c0-95bdbe542810">
          <AendringDefinition id="idcc050345-f3c2-491e-9426-bcacd9a89658">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 47, stk. 6,</Char>
                <Char> ændres »stk. 8 og 9« til: »stk. 7 og 8«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id0f719d28-d319-475c-a936-5ec47fe7dc62">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id07ffacd7-a8fe-4d68-bed5-f2dd1789665a">
        <Explicatus>2.</Explicatus>
        <Aendring id="id90447f62-9bb5-4f2b-a624-3e18a168a137">
          <AendringDefinition id="id2c25ae71-5d2e-4d64-bbc3-30bced39615a">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 47, stk. 7,</Char>
                <Char> ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id2d57e1c5-6847-4060-bd14-094233b4c038">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="id9a91d136-430b-4eb0-aea7-473c896a216a">
          <Exitus>
            <Linea>
              <Char>Stk. 8-12 bliver herefter stk. 7-11.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer id="id6e46993e-5f27-425c-8728-bdf3abe6a856">
