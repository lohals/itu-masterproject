        <Aendring id="ida87ed5a8-56bc-4fd3-81bc-9afca1462c63">
          <AendringDefinition id="id2bcc704e-5651-47e6-a766-43e9fb3a8ba2">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 82 </Char>
                <Char>indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 3:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
