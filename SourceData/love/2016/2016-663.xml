        <Aendring id="idc9bb0ac0-c4cd-4571-9d8f-ef4d71378159">
          <AendringDefinition id="ide51a6f09-8824-4915-bb18-0a31f734b9c8">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 94, stk. 4, </Char>
                <Char>indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">4. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idb0773613-a87c-44c2-ac60-da0ac0c49c2d">
            <AendringNyTekst>
              <Stk id="ida2abbeea-caf5-4caf-b083-b1c2cdab5615">
                <Exitus>
                  <Linea>
                    <Char>Udlændinge-, integrations- og boligministeren kan, når særlige forhold taler herfor, dispensere fra kravet i 1. pkt.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
