    <Ikraft id="id521e154b-2085-4735-bfb7-26d83e23883b">
      <Exitus>
        <Linea>
          <Char>Lov nr. 718 af 25. juni 2010 om ændring af konkursloven og forskellige andre love (Rekonstruktion m.v.)</Char>
        </Linea>
        <Nota id="id54ab6e19-f29c-4bc0-988a-50cd0822be71">
          <Explicatus>5)</Explicatus>
          <Exitus>
            <Linea>
              <Char>Ændringslovens § 14 vedrører § 3, stk. 1, nr. 4, og § 17, stk. 2, 1. pkt.</Char>
            </Linea>
          </Exitus>
        </Nota>
        <Linea>
          <Char>indeholder følgende ikrafttrædelsesbestemmelse:</Char>
        </Linea>
      </Exitus>
      <IkraftCentreretParagraf id="id2ebaa669-9e16-4000-a0ea-802adc75b471">
        <Explicatus>§ 55</Explicatus>
        <Stk id="idcf4f82c2-00a6-40fe-82c6-34ce074fcdc8">
          <Explicatus>Stk. 1. </Explicatus>
          <Exitus>
            <Linea>
