    <AendringCentreretParagraf id="id16ae7723-f178-4dc3-8af6-31e07a0f3f6b">
      <Explicatus>§ 4</Explicatus>
      <Exitus>
        <Linea>
          <Char>I kriminallov for Grønland, lov nr. 306 af 30. april 2008, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id6d488b50-57be-4199-b7da-53a161be52eb">
        <Explicatus>1.</Explicatus>
        <Aendring id="id30b6a49a-84ad-4867-b117-7081f39e35c8">
          <AendringDefinition id="id5cb30abd-0fe1-4be0-abbb-3e535768e463">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 76 </Char>
                <Char>indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 2:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id6f2c7028-d561-4523-aaa5-75e0cb5e5c96">
            <AendringNyTekst>
              <Stk id="id3faff635-6638-49df-a9c5-12172d580735">
                <Explicatus>Stk. 2.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Ligeledes dømmes den, der ulovligt fører barnet ud af riget.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
