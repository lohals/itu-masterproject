            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 631, stk. 1, </Char>
                <Char>indsættes efter 2. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id6061d319-fd7c-46f9-8245-1b237fde6cdc">
            <AendringNyTekst>
              <Stk id="id1dad7735-cc1a-456f-87c2-e8d4f17bd9be">
                <Exitus>
                  <Linea>
                    <Char>Samvær med andre end barnets forældre kan kun fuldbyrdes ved anvendelse af tvangsbøder.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
