          <AendringDefinition id="idf267cec6-f7cd-4c48-8245-a857601d7e44">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 599, stk. 5, </Char>
                <Char>affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id1a3a212a-70e2-4ea8-beef-83c2251b4159">
            <AendringNyTekst>
              <Stk id="id078dab9f-5f95-4c63-a73f-c903bc9ee28c">
                <Explicatus>Stk. 5.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Aftaler og afgørelser om forældremyndighed, barnets bopæl og samvær, jf. § 598, stk. 1, nr. 1-4, kan fuldbyrdes straks, medmindre andet er aftalt eller bestemt.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
