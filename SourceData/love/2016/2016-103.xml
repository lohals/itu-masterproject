            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 280, nr. 2-6, </Char>
                <Char>affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id222410ae-0b45-4b59-bfa2-956e73c1f7a1" punktumEfterSlutcitationstegn="True">
            <AendringNyTekst>
              <Indentatio id="id9c43dabd-6509-4ca5-991d-2437f907e21f" formaInd="Nummer">
                <Explicatus>2)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>sager om forældremyndighed,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
              <Indentatio id="id74388a4f-d1ca-49cc-882f-ea286d78c709" formaInd="Nummer">
                <Explicatus>3)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>sager om barnets bopæl,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
              <Indentatio id="idbfd23dca-1a82-4e30-9590-8d4a09e0a867" formaInd="Nummer">
                <Explicatus>4)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>sager om samvær,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
              <Indentatio id="idba47c893-3cf5-4a9b-bdf6-78fd32c87f28" formaInd="Nummer">
                <Explicatus>5)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>sager om anden kontakt,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
              <Indentatio id="ide0c344ec-9a28-459d-8de6-338e33ec76be" formaInd="Nummer">
                <Explicatus>6)</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>sager om tilladelse til udlandsrejse,</Char>
                  </Linea>
                </Exitus>
              </Indentatio>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id71043da1-5934-4f90-85b7-8c5bf0c43f06">
        <Explicatus>3.</Explicatus>
        <Aendring id="id621101d0-337c-4ffe-b968-a480032e69eb">
