          <AendringDefinition id="idafe2717a-dfd9-4594-96d0-f919c5a62c8e">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 631 </Char>
                <Char>indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 4:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id02271769-1446-463f-ba9e-33bf5c5091db">
            <AendringNyTekst>
              <Stk id="id1632d8ed-d140-4511-9028-55e824634699">
                <Explicatus>Stk. 4.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Et barn, som har den fornødne alder og modenhed, skal under en samtale have mulighed for at give udtryk for sine egne synspunkter, medmindre det er til skade for barnet.</Char>
                  </Linea>
                  <Linea>
                    <Char>Afholdes en samtale med barnet, skal der deltage en børnesagkyndig eller en repræsentant for kommunen.</Char>
                  </Linea>
                  <Linea>
                    <Char>§ 287 finder tilsvarende anvendelse.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id160b9ae4-11d4-47d7-af1e-6118fcf4eb73">
