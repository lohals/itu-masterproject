        <Aendring id="id11c6b998-2dca-4339-81dc-b718c3dda2c6">
          <AendringDefinition id="id5b17f2f7-480d-4333-99e3-1ba75e0fec55">
            <Exitus>
              <Linea>
                <Char formaChar="Italic">§ 287, 1. pkt., </Char>
                <Char>affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ide11e5215-fea7-4df2-94ab-ce70056749ae">
            <AendringNyTekst>
              <Stk id="id722e0ffc-6b08-4810-b8b7-04c5d5845a73">
                <Exitus>
                  <Linea>
                    <Char>Retten kan bestemme, at en eller begge parter ikke må overvære en samtale med et barn, der skal finde sted efter forældreansvarsloven.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
