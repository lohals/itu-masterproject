          <AendringDefinition id="id44d3e1c2-40f3-4bd2-9236-5fb1e8b882de">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic">§ 28</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 5:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id9ecf3538-5821-4484-9931-e53a7dee9c6d">
            <AendringNyTekst>
              <Stk id="ida629dfa4-22f2-4cbc-a7a2-39db9565329a">
                <Explicatus>Stk. 5.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Transport- og bygningsministeren kan fastsætte nærmere regler om opkrævning af gebyrer, herunder fastsætte størrelsen af det mindre, faste gebyr efter stk. 1, for kommunalbestyrelsens behandling af ansøgninger i henhold til loven.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
