    <AendringCentreretParagraf id="id9e4a156d-9b03-421e-a294-7c223e9ce378">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om almene boliger m.v., jf. lovbekendtgørelse nr. 1278 af 18. november 2015, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id06478f48-7af7-463a-ac94-240afd5657e5">
        <Explicatus>1.</Explicatus>
        <Aendring id="id5fe22b7e-7621-4a51-a23a-f84799beb1ef">
          <AendringDefinition id="id709a8d52-60cd-4935-86d7-dfbd9ce308f2">
            <Exitus>
              <Linea>
                <Char>I</Char>
                <Char formaChar="Italic"> § 51, stk. 3, 2. pkt.,</Char>
                <Char> og </Char>
                <Char formaChar="Italic">§ 185, stk. 1, nr. 3, </Char>
                <Char>ændres »selskabet« til: »boligorganisationen«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idc01dced5-1558-4064-87f1-9212b8f0223c">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="ida508a68c-6636-4a44-a805-be0f72e57684">
        <Explicatus>2.</Explicatus>
        <Aendring id="idf0230e5c-4578-4061-9bde-454af32df526">
          <AendringDefinition id="idb773d855-5b9b-4586-8b64-71ee20873e4a">
            <Exitus>
              <Linea>
                <Char>Overskriften før § 68 ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id1290d65e-c4fd-4043-904f-c9494402f06a">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id763cd098-700a-4408-8ee4-4eee86e39404">
        <Explicatus>3.</Explicatus>
        <Aendring id="iddc3e5160-d1ea-4965-bbd5-c5324b08b479">
          <AendringDefinition id="iddd14569d-7771-420e-b0a7-ecc6a3982ec4">
            <Exitus>
              <Linea>
                <Char>I</Char>
                <Char formaChar="Italic"> § 92 a, stk. 1, 1. pkt.,</Char>
                <Char> ændres »§ 10, stk. 2, i lov om friplejeboliger« til: »§ 10, stk. 1, i lov om friplejeboliger, jf. lovbekendtgørelse nr. 897 af 17. august 2011, eller efter § 11 a i lov om friplejeboliger«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id23024d69-4c5a-4194-a0ea-7ecfe0796f3e">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id79bb506e-c9a0-4e6e-a48f-c93abdbecca3">
