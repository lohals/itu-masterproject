  <DokumentIndhold id="id4ea6a336-d14f-4fd2-b841-c3bdca60b099">
    <Hymne id="id1413c69b-3f7f-445b-9994-f42d14300638">
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf id="id47b14773-c3ee-469a-8a20-a00c4f3c0883">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov for Grønland om visse spil, jf. lovbekendtgørelse nr. 336 af 7. april 2016, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id440aadd6-2525-4ed6-8c11-b930fe7eec4b">
        <Explicatus>1.</Explicatus>
        <Aendring id="idd5bc534f-7498-4dbc-bfae-830609326930">
          <AendringDefinition id="id91bc2499-5ffd-41b9-9ee1-c6bcd47000c8">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 35, stk. 1, 2. pkt.,</Char>
                <Char> affattes således:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idc66559c8-5919-43c4-aa60-6d485e1a7fb7">
            <AendringNyTekst>
              <Exitus formaEx="Align">
                <Linea>
                  <Char>Beløbet fastsættes til 43.250.000 kr. (2015-niveau).</Char>
                </Linea>
              </Exitus>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="idae225fa9-1c9e-4cd0-ab0a-2dad384f3f94">
        <Explicatus>2.</Explicatus>
        <Aendring id="id1a0fe588-78d9-48b0-9c92-b368799eb1a5">
          <AendringDefinition id="idb1d6e07a-4f44-4459-87be-7578452614d9">
            <Exitus>
              <Linea>
                <Char signiChar="AendringURN" formaChar="Italic">§ 35, stk. 3,</Char>
                <Char> ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id139b1b42-fc4e-48dd-904f-ce50fb78aa94">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="id430e5afe-d030-42dc-ac8f-75e114d722ee">
          <Exitus>
            <Linea>
              <Char>Stk. 4 og 5 bliver herefter stk. 3 og 4.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer id="id9dbf5da0-bcab-4f54-9d07-1d3f413068a5">
        <Explicatus>3.</Explicatus>
        <Aendring id="id1e4efd37-4533-459d-b514-c8e0240005b6">
          <AendringDefinition id="id42b744d3-d773-45d1-88a5-94b70ccd8f67">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 35, stk. 4,</Char>
                <Char> der bliver stk. 3, ændres »jf. dog stk. 5.« til: »jf. dog stk. 4.«</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id58d1f658-6164-4be0-8307-d5beb37ef213">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="idca3e3035-b336-44af-b430-292384984caf">
      <Explicatus>§ 2</Explicatus>
      <Stk id="idc13c627e-3b6b-4ce4-a963-18ab6c97ed03">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2017.</Char>
          </Linea>
        </Exitus>
      </Stk>
