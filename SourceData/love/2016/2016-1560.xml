    <AendringCentreretParagraf id="id224243ff-cd2c-46f4-a9f6-381f5370cff0">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I postloven, lov nr. 1536 af 21. december 2010, som ændret ved lov nr. 172 af 26. februar 2014 og § 46 i lov nr. 524 af 29. april 2015, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="idc6f64669-d7cf-4bb2-89bd-0bafb77d6bfb">
        <Explicatus>1.</Explicatus>
        <Aendring id="idee647a67-8f31-4e28-8180-6431c85e33b7">
          <AendringDefinition id="id5e543261-3fc7-4201-9e7f-b93c6215f723">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 3, stk. 4,</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">2. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id9c4274f7-386d-4692-9230-d7a571c1521a">
            <AendringNyTekst>
              <Stk id="id11ee751b-39b3-44bb-8802-3ef469a299d9">
                <Exitus>
                  <Linea>
                    <Char>1. pkt. gælder dog ikke for tilladelser, som udstedes efter den 1. januar 2020.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="id8e5b8f8c-de75-4fe0-82b3-ed804996512b">
        <Explicatus>2.</Explicatus>
        <Aendring id="idbeaeb8da-1531-466e-bb4f-6fc974900f1f">
          <AendringDefinition id="id7d710268-0188-43c8-a4f6-66b165eb860e">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 15, stk. 4,</Char>
                <Char> ændres »B-breve« til: »breve«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idfdb6c3b2-b919-4144-b989-4d6aa888e53a">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
