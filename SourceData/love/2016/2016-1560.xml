        <Explicatus>3.</Explicatus>
        <Aendring id="idf485b936-655c-4a6a-a3ee-4e81c07b9839">
          <AendringDefinition id="idd1f2feb7-c218-42fd-b3ed-3a4ac3e7d7c7">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 18, stk. 1,</Char>
                <Char> indsættes efter 1. pkt.:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id27d487b0-f028-46b3-9830-e76b50ced265">
            <AendringNyTekst>
              <Stk id="id7ad019db-c1c1-4249-9e7b-1f79d302fc7c">
                <Exitus>
                  <Linea>
                    <Char>Det gælder nettoomkostninger til og med regnskabsåret 2016 hos den befordringspligtige postvirksomhed.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="id03035073-daf0-46cd-a8e9-a44b65af4e01">
      <Explicatus>§ 2</Explicatus>
      <Stk id="id52943e33-b37e-4e5f-8c84-f53a67451bd5">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2017.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="idc773012e-94f9-4123-b65e-25b8528c0e79">
