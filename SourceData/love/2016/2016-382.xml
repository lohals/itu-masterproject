  </TitelGruppe>
  <DokumentIndhold id="id3accc68d-d16c-4a4f-850e-bbde0eca57cb">
    <Hymne id="idaa4519fd-d720-4b1d-8fed-0dd0036bb463">
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf id="id04813cbc-d362-4785-812d-1f3fa80cadf9">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om euforiserende stoffer, jf. lovbekendtgørelse nr. 748 af 1. juli 2008, som ændret ved lov nr. 163 af 28. februar 2012 og lov nr. 606 af 18. juni 2012, foretages følgende ændring:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id7356a233-a4fe-4929-b97d-1cb8b7c89c3c">
        <Explicatus>1.</Explicatus>
        <Aendring id="idee78b1dc-e269-4522-afd0-26f1ded7e7c3">
          <AendringDefinition id="id82c350d1-1efa-41b8-9b64-ce3191163d8c">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic">§ 3 b, stk. 2,</Char>
                <Char> indsættes efter »personale,«: »stofindtagelse,«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="idb606503b-b63b-4938-92bc-d136a9f04274">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="id7f1df673-965b-42da-9443-1cb6b97d3a02">
      <Explicatus>§ 2</Explicatus>
      <Stk id="id6976a354-be03-4647-8eb7-10dd4c75db46">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. juli 2016.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="idf9c4b69d-a091-4f23-a837-6e41a282aa0d">
      <Explicatus>§ 3</Explicatus>
      <Stk id="idecd1d91c-6880-45ec-9a6f-a1cac548069b">
        <Explicatus>Stk. 1.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland, jf. dog stk. 2.</Char>
          </Linea>
        </Exitus>
      </Stk>
      <Stk id="id0659759e-8df8-4c37-8f85-6893e3fa4b37">
        <Explicatus>Stk. 2.</Explicatus>
        <Exitus>
          <Linea>
            <Char>Loven kan ved kongelig anordning sættes i kraft for Færøerne med de ændringer, som de færøske forhold tilsiger.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
