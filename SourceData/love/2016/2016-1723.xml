  </TitelGruppe>
  <DokumentIndhold id="idc715629d-5ebb-4ee6-8ada-dc82e3af311a">
    <Hymne id="id65a8a368-a72e-4c0d-b176-5eb3ef6d647e">
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf id="idd04c419e-1365-4c38-a2e7-ae7707479e98">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I straffeloven, jf. lovbekendtgørelse nr. 1052 af 4. juli 2016, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="id6f70b06d-c17b-48b3-bfb3-82c400663ca1">
        <Explicatus>1.</Explicatus>
        <Aendring id="id518c4712-15b6-4719-bf1e-cf2661fe7119">
          <AendringDefinition id="id793f7a7d-b82a-4b97-87f2-fa5ad0b39582">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 136</Char>
                <Char> indsættes som </Char>
                <Char signiChar="AendringURN" formaChar="Italic">stk. 3:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="ida0140f74-de4e-43ae-b8e8-d33fe5ea4f37">
            <AendringNyTekst>
              <Stk id="id9f325b37-808e-40c8-a26a-f670d6990996">
                <Explicatus>Stk. 3.</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>Den, der som led i religiøs oplæring udtrykkeligt billiger handlinger, som er omfattet af denne lovs §§ 114-114 j, 208, 210, 216, 222, 223 og 225, jf. §§ 216, 222 og 223, §§ 237, 244-246, 260 og 261, straffes med bøde eller fængsel indtil 3 år.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="id6338848a-294a-4b67-86d4-8bedd3942ba6">
      <Explicatus>§ 2</Explicatus>
      <Stk id="idd9cbe040-6f1a-44f6-a235-c65a22ceb01a">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. januar 2017.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="id7cdf9fbf-e3de-4fe8-af79-7923196fe2d7">
      <Explicatus>§ 3</Explicatus>
      <Stk id="ida9cb3359-86c2-49ef-addf-0d83b27396c7">
        <Exitus>
          <Linea>
            <Char>Justitsministeren fremsætter forslag om revision af loven i folketingsåret 2021-22.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
