      </AendringsNummer>
      <AendringsNummer id="id830a6be8-27e4-4189-9958-17fd6ac376b4">
        <Explicatus>7.</Explicatus>
        <Aendring id="id6fc64b91-b7dd-4eea-9c02-5badeb698b5f">
