  </TitelGruppe>
  <DokumentIndhold id="idae934d27-7138-4b8b-9ff9-8fcdec1341b3">
    <Hymne id="idd19bed57-01e8-446b-891f-003921c55aa2">
      <Exitus>
        <Linea>
          <Char>VI MARGRETHE DEN ANDEN, af Guds Nåde Danmarks Dronning, gør vitterligt:</Char>
        </Linea>
      </Exitus>
      <Exitus>
        <Linea>
          <Char>Folketinget har vedtaget og Vi ved Vort samtykke stadfæstet følgende lov:</Char>
        </Linea>
      </Exitus>
    </Hymne>
    <AendringCentreretParagraf id="id6c0f7868-823d-4d8a-977b-5f0d134bb607">
      <Explicatus>§ 1</Explicatus>
      <Exitus>
        <Linea>
          <Char>I lov om efterskoler og frie fagskoler, jf. lovbekendtgørelse nr. 1076 af 8. juli 2016, som ændret ved § 2 i lov nr. 1563 af 13. december 2016, foretages følgende ændringer:</Char>
        </Linea>
      </Exitus>
      <AendringsNummer id="idfe2ac0f0-4880-4ab5-a484-28fc4381da46">
        <Explicatus>1.</Explicatus>
        <Aendring id="id968f35cb-caf0-4ea9-a095-f15865c6f125">
          <AendringDefinition id="idffbb3195-c097-4276-9cb9-88f357e7b659">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 14</Char>
                <Char> indsættes efter stk. 1 som nyt stykke:</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id4294885a-cd74-4a4f-bd0a-08a821fea0b6">
            <AendringNyTekst>
              <Stk id="id20ac8a74-17e2-4acf-8e8a-82698ef100d2">
                <Explicatus>Stk. 2,</Explicatus>
                <Exitus>
                  <Linea>
                    <Char>En elev kan højst indgå i beregningen af en efterskoles årselevtal i 42 uger inden for samme skoleår.</Char>
                  </Linea>
                </Exitus>
              </Stk>
            </AendringNyTekst>
          </AendringAktion>
        </Aendring>
        <Rykningsklausul id="idd9fa793c-fa6a-43ff-812a-ea37b1846f29">
          <Exitus>
            <Linea>
              <Char>Stk. 2 og 3 bliver herefter stk. 3 og 4.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer id="id047f4185-849e-474b-a804-94adb1074925">
        <Explicatus>2.</Explicatus>
        <Aendring id="id6b754fc7-7269-4900-88e8-abf8f7016b21">
          <AendringDefinition id="id1d21c5a6-b1de-48cd-8bc4-23491477da8d">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 30, stk. 2, </Char>
                <Char>ændres</Char>
                <Char formaChar="Italic">
                </Char>
                <Char>»opnå elevstøtte,« til: »opnå elevstøtte i højst 42 uger i et skoleår,«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id8ece3248-49ce-4a05-980d-59ad4594bd5c">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
    </AendringCentreretParagraf>
    <IkraftCentreretParagraf id="id68fecfbe-bb0c-4dde-83d6-4f2ab5b38238">
      <Explicatus>§ 2</Explicatus>
      <Stk id="id3894944d-bc85-4d29-b522-48abcf630d35">
        <Exitus>
          <Linea>
            <Char>Loven træder i kraft den 1. august 2017.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
    <IkraftCentreretParagraf id="id63db83c3-04f6-44cc-853a-a8ad5c9db248">
      <Explicatus>§ 3</Explicatus>
      <Stk id="idd8252444-3785-4680-8ede-103d1b44e2ab">
        <Exitus>
          <Linea>
            <Char>Loven gælder ikke for Færøerne og Grønland.</Char>
          </Linea>
        </Exitus>
      </Stk>
    </IkraftCentreretParagraf>
