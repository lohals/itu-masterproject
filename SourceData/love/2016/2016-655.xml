          <AendringDefinition id="id86d70f15-782d-4cad-b99b-3eb107a4d514">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 4, stk. 2, </Char>
                <Char>der bliver stk. 3, ændres »jf. dog § 5« til: »når betingelserne for tilbageholdelsen efter § 5 ikke er opfyldt«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion id="id4928abfb-afc0-4efd-9f47-e41c9e86f061">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer id="idb179ac30-54f2-49ca-bfa5-84d0e3460f31">
        <Explicatus>6.</Explicatus>
        <Aendring id="idba22129c-97d4-4190-ba62-d58d7320abce">
          <AendringDefinition id="id2748075c-6794-45ba-9377-b6fc51747514">
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char signiChar="AendringURN" formaChar="Italic">§ 5, stk. 1, nr. 1, </Char>
                <Char>ændres »forringet eller« til: »forringet,«.</Char>
              </Linea>
