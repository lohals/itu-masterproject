      <AendringsNummer>
        <Explicatus>4.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 4 A, stk. 4, 1. pkt.,</Char>
