                <Char>I </Char>
                <Char formaChar="Italic" signiChar="AendringURN">§ 22, stk., 1, nr. 5,</Char>
                <Char> ændres »husholdningsskole,« til: »fri fagskole eller«.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>3.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char formaChar="Italic" signiChar="AendringURN">§ 22, stk. 1, nr. 6,</Char>
                <Char> ophæves.</Char>
              </Linea>
            </Exitus>
          </AendringDefinition>
          <AendringAktion punktumEfterSlutcitationstegn="False">
            <AendringNyTekst />
          </AendringAktion>
        </Aendring>
        <Rykningsklausul>
          <Exitus>
            <Linea>
              <Char>Nr. 7 bliver herefter nr. 6.</Char>
            </Linea>
          </Exitus>
        </Rykningsklausul>
      </AendringsNummer>
      <AendringsNummer>
        <Explicatus>4.</Explicatus>
        <Aendring>
          <AendringDefinition>
            <Exitus>
              <Linea>
                <Char>I</Char>
                <Char formaChar="Italic" signiChar="AendringURN"> § 22, stk. 4,</Char>
                <Char> ændres »nr. 3-7,« til: »nr. 3-6,«.</Char>
              </Linea>
